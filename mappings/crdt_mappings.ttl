@prefix crdt: <https://kkalass.github.io/solid_crdt_sync/vocab/crdt#> .
@prefix sync: <https://kkalass.github.io/solid_crdt_sync/vocab/sync#> .
@prefix idx: <https://kkalass.github.io/solid_crdt_sync/vocab/idx#> .
@prefix : <https://kkalass.github.io/solid_crdt_sync/mappings/> .

:partitioned-index-v1 a sync:ClassMapping;
   sync:appliesToClass idx:PartitionedIndex;
   sync:propertyMapping
     [ sync:property idx:indexesClass; crdt:mergeWith crdt:LWW_Register ],
     [ sync:property idx:indexedProperty; crdt:mergeWith crdt:LWW_Register ],
     [ sync:property idx:shardingAlgorithm; crdt:mergeWith crdt:LWW_Register ],
     [ sync:property idx:partitionedBy; crdt:mergeWith crdt:LWW_Register ].